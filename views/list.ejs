<%- include('header'); -%>
    <div id="heading" class="box">
        <h1>It's <%= listTitle %></h1>
    </div>

    <div class="box">
        <% for (let i = 0; i<todo.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= todo[i] %></p>
        </div>
        <% } %>
        <form class="item" action="/" method="post">
            <input class="focus" type="text" name="todo" placeholder="Add to do" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
    
    
<%- include('footer') -%>